@article{filstrupEvidenceRegionalNitrogen2018,
  ids = {filstrupEvidenceRegionalNitrogen2018a},
  title = {Evidence for Regional Nitrogen Stress on Chlorophyll {\emph{a}} in Lakes across Large Landscape and Climate Gradients: {{N}} Stress on Lake Phytoplankton},
