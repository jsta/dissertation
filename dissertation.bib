@article{thomaTillageNutrientSource2005,
  title = {Tillage and {{Nutrient Source Effects}} on {{Water Quality}} and {{Corn Grain Yield}} from a {{Flat Landscape}}},
  author = {Thoma, David P. and Gupta, Satish C. and Strock, Jeffrey S. and Moncrief, John F.},
  year = {2005},
  volume = {34},
  pages = {1102},
  issn = {1537-2537},
  doi = {10.2134/jeq2004.0200},
  abstract = {Beneficial effects of leaving residue at the soil surface are well documented for steep lands, but not for flat lands that are drained with surface inlets and tile lines. This study quantified the effects of tillage and nutrient source on tile line and surface inlet water quality under continuous corn (Zea mays L.) from relatively flat lands (Ͻ3\%). Tillage treatments were either fall chisel or moldboard plow. Nutrient sources were either fall injected liquid hog manure or spring incorporated urea. The experiment was on a Webster\textendash Canisteo clay loam (Typic Endoaquolls) at Lamberton, MN. Surface inlet runoff was analyzed for flow, total solids, NO3\textendash N, NH4\textendash N, dissolved P, and total P. Tile line effluent was analyzed for flow, NO3\textendash N, and NH4\textendash N. In four years of rainstorm and snowmelt events there were few significant differences ( p Ͻ 0.10) in water quality of surface inlet or tile drainage between treatments. Residue cover minimally reduced soil erosion during both snowmelt and rainfall runoff events. There was a slight reduction in mineral N losses via surface inlets from manure treatments. There was also a slight decrease ( p ϭ 0.025) in corn grain yield from chisel-plow plots (9.7 Mg haϪ1) compared with moldboardplow plots (10.1 Mg haϪ1). Chisel plowing (approximately 30\% residue cover) alone is not sufficient to reduce nonpoint source sediment pollution from these poorly drained flat lands to the extent (40\% reduction) desired by regulatory agencies.},
  file = {/home/julie/Zotero/storage/927F4W2C/Thoma et al. - 2005 - Tillage and Nutrient Source Effects on Water Quali.pdf},
  journal = {Journal of Environment Quality},
  language = {en},
  number = {3}
}

@article{tomerEvaluationNitrateNitrogen2003,
  title = {Evaluation of {{Nitrate Nitrogen Fluxes}} from a {{Tile}}-{{Drained Watershed}} in {{Central Iowa}}},
  author = {Tomer, M D and Meek, D W and Jaynes, D B and Hatfield, J L},
  year = {2003},
  volume = {32},
  pages = {12},
  abstract = {Nitrate N fluxes from tile-drained watersheds have been implicated in water quality studies of the Mississippi River basin, but actual NO3\textendash N loads from small watersheds during long periods are poorly documented. We evaluated discharge and NO3\textendash N fluxes passing the outlet of an Iowa watershed (5134 ha) and two of its tile-drained subbasins (493 and 863 ha) from mid-1992 through 2000. The cumulative NO3\textendash N load from the catchment was 168 kg haϪ1, and 176 and 229 kg haϪ1 from the subbasins. The outlet had greater total discharge (1831 mm) and smaller flow-weighted mean NO3\textendash N concentration (9.2 mg LϪ1) than the subbasins, while the larger subbasin had greater discharge (1712 vs. 1559 mm) and mean NO3\textendash N concentration (13.4 vs. 11.3 mg LϪ1) than the smaller subbasin. Concentrations exceeding 10 mg LϪ1 were common, but least frequent at the outlet. Nitrate N was generally not diluted by large flows, except during 1993 flooding. The outlet showed smaller NO3\textendash N concentrations at low flows. Relationships between discharge and NO3\textendash N flux showed log\textendash log slopes near 1.0 for the subbasins, and 1.2 for the outlet, considering autocorrelation and measurement-error effects. We estimated denitrification of subbasin NO3\textendash N fluxes in a hypothetical wetland using published data. Assuming that temperature and NO3\textendash N supply could limit denitrification, then about 20\% of the NO3\textendash N would have been denitrified by a wetland constructed to meet USDA-approved criteria. The low efficiency results from the seasonal timing and NO3\textendash N content of large flows. Therefore, agricultural and wetland best management practices (BMPs) are needed to achieve water quality goals in tiledrained watersheds.},
  file = {/home/julie/Zotero/storage/CTZNQ6PP/Tomer et al. - 2003 - Evaluation of Nitrate Nitrogen Fluxes from a Tile-.pdf},
  journal = {J. ENVIRON. QUAL.},
  language = {en}
}

