@article{wellerCroplandRiparianBuffers2014,
  title = {Cropland {{Riparian Buffers}} throughout {{Chesapeake Bay Watershed}}: {{Spatial Patterns}} and {{Effects}} on {{Nitrate Loads Delivered}} to {{Streams}}},
