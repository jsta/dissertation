@article{renwickStreamNitrogenPhosphorus2018,
  title = {Stream {{Nitrogen}}, {{Phosphorus}}, and {{Sediment Concentrations Show Contrasting Long}}-Term {{Trends Associated}} with {{Agricultural Change}}},
